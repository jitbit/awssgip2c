{
  "manifest_version": 2,

  "name": "AWS Sec Grp IP2Location",
  "description": "Shows IP location for EC2security groups IP",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Find Countries"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "https://freegeoip.net/"
  ]
}